{
  "name": "netpipobackend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --require dotenv/config  src/index.ts",
    "migrate": "npx sequelize-cli db:migrate",
    "seed": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo:all",
    "migrate:reset": "npx sequelize-cli db:migrate:undo:all && npm run migrate && npm run seed",
    "run-tests": "cross-env NODE_ENV=test jest --detectOpenHandles --coverage --verbose --testTimeout=10000",
    "run-after-tests": "cross-env NODE_ENV=test sequelize db:migrate:undo:all ",
    "pretest": "cross-env NODE_ENV=test npm run migrate:reset",
    "test": "npm-run-all  run-tests run-after-tests --continue-on-error"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/supertest": "^6.0.2",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "sequelize-cli": "^6.6.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.3"
  },
  "jest": {
    "preset": "ts-jest",
    "setupFiles": [
      "<rootDir>/jest.setup.ts"
    ],
    "testEnvironment": "node",
    "coverageReporters": [
      "json-summary",
      "text",
      "lcov"
    ],
    "coveragePathIgnorePatterns": [
      "node_modules/",
      "/src/database/config",
      "src/documention/index.ts",
      "src/middlewares",
      "./package-lock.json"
    ]
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.5"
  }
}
